{
  "CoreBot": {
    "Llm": {
      "Provider": "openrouter",
      "ApiKey": "${OPENROUTER_API_KEY}",
      "Model": "anthropic/claude-3.5-sonnet",
      "MaxTokens": 4096,
      "Temperature": 0.7
    },
    "ChatPlatforms": {
      "telegram": {
        "Enabled": true,
        "ApiKey": "${TELEGRAM_BOT_TOKEN}",
        "Settings": {}
      },
      "whatsapp": {
        "Enabled": false,
        "ApiKey": "${WHATSAPP_API_KEY}",
        "Settings": {}
      },
      "feishu": {
        "Enabled": false,
        "ApiKey": "${FEISHU_API_KEY}",
        "Settings": {}
      }
    },
    "Tools": {
      "WorkspacePath": "~/.nanobot/workspace",
      "ShellTimeoutSeconds": 30,
      "AllowedTools": [
        "file_read",
        "file_write",
        "shell",
        "web_fetch",
        "send_message"
      ]
    },
    "Scheduler": {
      "Tasks": []
    },
    "Logging": {
      "Level": "Information"
    }
  }
}
